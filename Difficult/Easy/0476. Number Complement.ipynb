   "source": [
    "class Solution:\n",
    "    def findComplement(self, num: 'int') -> 'int':\n",
    "        s=bin(num)[2:]\n",
    "        s1=''\n",
    "        for i in range(len(s)):\n",
    "            if s[i]=='0':\n",
    "                s1+='1'\n",
    "            else:\n",
    "                s1+='0'\n",
    "        return int(s1,2)"
   ]
  }
