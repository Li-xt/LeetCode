    "class Solution(object):\n",
    "    def titleToNumber(self, s):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        sum=0\n",
    "        for c in s:\n",
    "            sum=sum*26+(ord(c)-64)\n",
    "        return sum"
   ]
